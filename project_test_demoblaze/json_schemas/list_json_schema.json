{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "Items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "cat": {
            "type": "string",
            "enum": [
              "monitor",
              "laptops",
              "phones"
            ]
          },
          "desc": {
            "type": "string"
          },
          "id": {
            "type": "integer"
          },
          "img": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "title": {
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": [
          "cat",
          "desc",
          "id",
          "img",
          "price",
          "title"
        ]
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "Items"
  ]
}